# Environment
NODE_ENV=development              # or "production" depending on your deployment

# Server
HTTP=http://localhost:3000
PORT=3000

# Feature flags
SWAGGER=true

# Timeouts and limits
TIMEOUT=30

# Rate limiting (separate vars)
RATE_LIMIT_TIME_SPAN=60
RATE_LIMIT_ALLOWED_CALLS=100

# Memoization and cache
MEMOIZE_TIME=300
CACHE_SIZE=1000